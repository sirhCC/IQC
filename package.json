{
  "name": "infrastructure-query-language",
  "version": "0.1.0",
  "description": "SQL-like query language for infrastructure - query across clouds, K8s, CI/CD, and more",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "iql": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "infrastructure",
    "devops",
    "query",
    "sql",
    "cloud",
    "kubernetes"
  ],
  "author": "sirhCC",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.943.0",
    "@aws-sdk/client-lambda": "^3.943.0",
    "@aws-sdk/client-rds": "^3.943.0",
    "@aws-sdk/credential-providers": "^3.943.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
